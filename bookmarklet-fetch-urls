javascript:(function(){var siteName=location.hostname,els=document.querySelectorAll("*"),urls=[];for(var i=0;i<els.length;i++){var e=els[i];if(e.src)urls.push(e.src);if(e.href)urls.push(e.href);}urls=[...new Set(urls)].sort();var textContent=urls.join("\n"),blob=new Blob([textContent],{type:"text/plain"}),objUrl=URL.createObjectURL(blob),a=document.createElement("a");a.href=objUrl;a.download=siteName+"_urls.txt";document.body.appendChild(a);a.click();document.body.removeChild(a);URL.revokeObjectURL(objUrl);})();
